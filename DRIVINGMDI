Private Sub Admission_Click()
 If DataEnvironment1.Connection1.State = 1 Then
DataEnvironment1.Connection1.Close
End If
DataEnvironment1.Connection1.Open
DataEnvironment1.Command2
  DataReport2.Show
End Sub
Private Sub AdmissionForm_Click()
    Add_Frm.Show
    Forms.Enabled = False
    Reports.Enabled = False
    File.Enabled = False
End Sub
Private Sub Exit_Click()
    Unload Me
End Sub
